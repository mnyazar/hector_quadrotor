<?xml version="1.0" encoding="UTF-8"?>
<!-- Simulate robot in gazebo: To load our robot into the empty world  -->
<launch>
    <param name="robot_description" command="cat '$(find hector_quadrotor_gazebo)/urdf/quadrotor.urdf'" />

    <arg name="x" default="0"/>
    <arg name="y" default="0"/>
    <arg name="z" default="0.3"/>

    <include file="$(find gazebo_ros)/launch/empty_world.launch">
    	    <arg name="world_name" value="$(find m2wr)/worlds/kitchen_dinning.world"/>
		    <arg name="paused" value="false"/>
		    <arg name="use_sim_time" value="true"/>
		    <arg name="gui" value="true"/>
		    <arg name="headless" value="false"/>
		    <arg name="debug" value="false"/>
    </include>

    <node name="mybot_spawn" pkg="gazebo_ros" type="spawn_model" output="screen"
          args="-urdf -param robot_description -model hector_quadrotor -x $(arg x) -y $(arg y) -z $(arg z)" />
</launch>